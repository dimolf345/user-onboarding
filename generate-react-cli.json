{
  "usesTypeScript": false,
  "usesCssModule": false,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/default/TemplateName.jsx",
        "style": "templates/default/TemplateName.style.scss",
        "test": "templates/default/TemplateName.test.jsx"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    },
    "withHooks": {
      "customTemplates": {
        "component": "templates/withHooks/TemplateName.jsx",
        "style": "templates/withHooks/TemplateName.style.scss",
        "test": "templates/withHooks/TemplateName.test.jsx"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    }
  }
}